{
    "name": "reach-digital/php-connector-lib",
    "description": "Library to create a connector with any eCommerce PHP Platform (M1/M2/etc.)",
    "type": "library",
    "license": "COMMERCIAL",
    "authors": [
        {
            "name": "Paul Hachmang",
            "email": "paul@h-o.nl"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "files" : [ "register.php" ],
        "psr-4": { "ReachDigital\\PhpConnectorLib\\": "./" }
    },
    "require": {
        "chrisboulton/php-resque": "dev-master",
        "chrisboulton/php-resque-scheduler": "dev-master",
        "ho-nl-fork/php-resque-retry": "dev-master",
        "jaaprood/php-resque-plugin": "dev-master"
    },
    "repositories": {
        "chrisboulton/php-resque-scheduler": {
            "type": "vcs",
            "url": "git@github.com:chrisboulton/php-resque-scheduler.git"
        },
        "ho-nl-fork/php-resque-retry": {
            "type": "vcs",
            "url": "git@github.com:ho-nl-fork/php-resque-retry.git"
        },
        "jaaprood/php-resque-plugin": {
            "type": "vcs",
            "url": "git@github.com:JaapRood/php-resque-plugin.git"
        }
    }
}
